{
  "kind": "collectionType",
  "collectionName": "posts",
  "info": {
    "name": "Post"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "Meta": {
      "type": "component",
      "repeatable": false,
      "component": "page.meta"
    },
    "Content": {
      "type": "dynamiczone",
      "components": [
        "page.copy",
        "page.image"
      ]
    }
  }
}
